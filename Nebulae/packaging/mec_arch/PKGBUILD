pkgname=mec
pkgver=0.1.1
pkgrel=1
pkgdesc="See https://github.com/TheTechnobear/MEC/ for more information."
arch=('armv7h')
url="https://github.com/TheTechnobear/MEC/"
license=('GPL')
groups=('sound')
source=('mec.zip')
depends=('gcc-libs' 'glibc' 'libusb' 'cairo')
install=mec.install

package() {
    cd "${srcdir}/${pkgname}"
    cp -r . ${pkgdir}/
}
md5sums=('09528100ab5bf4c20399284b64f09226')
