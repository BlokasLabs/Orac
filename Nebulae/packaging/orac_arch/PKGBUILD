pkgname=orac
pkgver=0.2.1
pkgrel=1
pkgdesc="See https://github.com/TheTechnobear/Orac/ for more information."
arch=('armv7h')
url="https://github.com/TheTechnobear/Orac/"
license=('GPL')
groups=('sound')
source=('orac.zip')
optdepends=('mec: adds remote control for orac')
install=orac.install

package() {
    cd "${srcdir}/${pkgname}"
    cp -r . ${pkgdir}/
}
