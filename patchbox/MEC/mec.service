[Unit]
Description=Run MEC at boot
DefaultDependencies=no
Wants=network-online.target
After=network-online.target
Conflicts=shutdown.target
Before=shutdown.target 

[Service]
Type=simple
User=patch
Group=patch
Environment=HOME=/home/patch
KillMode=process
Restart=always
WorkingDirectory=/usr/local/MEC
ExecStart=/usr/local/MEC/mec-app osc_kontrol.json 
#ExecStart=/usr/local/MEC/mec-app push_kontrol.json 
#ExecStart=/usr/local/MEC/mec-app e1kontrol.json 

[Install]
WantedBy=default.target
